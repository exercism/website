set x to -1
set y to -1
set num_iterations to 0
fill_color_hex("#AA4A44")

set wide to true
repeat 10 times do
  change y to y + 1
  if wide is true do
    change x to -1
    change num_iterations to 5
  else do
    change x to -1.5
    change num_iterations to 6
  end
  change wide to !wide
  
  repeat num_iterations times do
    change x to x + 1
    rectangle(x * 20, y * 10, 20, 10)
  end
end