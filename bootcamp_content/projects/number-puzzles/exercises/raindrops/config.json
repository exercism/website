{
  "title": "Raindrop Sounds",
  "description": "Convert a number into a raindrop sound based on its factors.",
  "level": 4,
  "idx": 4,
  "tests_type": "io",
  "readonly_ranges": [
    { "from": 1, "to": 3 },
    { "from": 5, "to": 5 }
  ],
  "stdlib_functions": ["join", "number_to_string"],
  "tasks": [
    {
      "name": "Plings",
      "tests": [
        {
          "slug": "number-3",
          "description_html": "If 3 is input, return \"Pling\"",
          "name": "Number 3",
          "function": "raindrops",
          "params": [3],
          "expected": "Pling"
        },
        {
          "slug": "number-27",
          "description_html": "If 27 is input, return \"Pling\"",
          "type": "io",
          "name": "Number 27",
          "function": "raindrops",
          "params": [27],
          "expected": "Pling"
        }
      ]
    },
