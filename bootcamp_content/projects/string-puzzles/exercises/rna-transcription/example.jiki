function transform_strand with strand do
  set dna to ["A", "C", "G", "T"]
  set rna to ["U", "G", "C", "A"]

  set counter to 0
  for each letter in dna do
    change counter to counter + 1
    if strand == letter do
      return rna[counter]
    end
  end
end

function dna_to_rna with dna do
  set rna to ""
  for each letter in dna do
    change rna to join(rna, transform_strand(letter))
  end
  return rna
end