// Create the Clock class here


function railway_time with hours, minutes do
  set clock to new Clock(hours, minutes)
  return clock.get_railway_time()
end

function duodecimal_time with hours, minutes do
  set clock to new Clock(hours, minutes)
  return clock.get_duodecimal_time()
end


